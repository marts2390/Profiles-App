{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "@src/*": ["./src/*"],
      "@store/*": ["./src/store/*"],
      "@delivery/*": ["./src/delivery/*"],
      "@components/*": ["./src/components/*"]
    }
  },
  "include": ["./src/**/*"],
  "exclude": ["node_modules"]
}
